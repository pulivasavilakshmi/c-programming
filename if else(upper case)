#include <stdio.h>
int main() {
      char ch ;
      printf("enter ch = ");
      scanf("%c",& ch);
      if(ch >='A' && ch <= 'Z'){
        printf("Upper case");
      }
      else if(ch >= 'a' && ch <= 'z'){
        printf("Lower case");
      }
      else{
        printf("invalid case");
      }
    
    return 0;
}
