#include <stdio.h>
int main() {
   int x,y,z;
   printf("enter x = ");
   scanf("%d",& x);

   printf("enter y = ");
   scanf("%d",& y);

   printf("enter z = ");
   scanf("%d",& z);
     
     if(x + y + z == 180);{
        printf("valid triangle");
     }
     else {
      printf("not valid triangle");
}

    
    return 0;
}
