#include <stdio.h>
int main() {
   int x,y,z;
   printf("enter x = ");
   scanf("%d",& x);

   printf("enter y = ");
   scanf("%d",& y);

   printf("enter z = ");
   scanf("%d",& z);
     
     if(x == y && y == z && x == z){
        printf("equilaterial triangle");
     }
     else {
      printf("not equilaterial triangle");
}

    
    return 0;
}
